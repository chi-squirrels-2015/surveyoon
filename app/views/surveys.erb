  <h2> Survey Created </h2>

  <tr>
    <th>Name</th> 
    <th>edit</th>
    <th>stats</th>
  </tr>
    <tr>
    <%#@user.surveys.each do |survey| %> 	
    <td><a href="http://localhost.com:9393/survey/<%#=survey.id%>"><%#=survey.title%>Kevin Survey</a>
